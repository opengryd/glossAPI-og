# MinerU environment â€” generated by setup_glossapi.sh
#
# Copy this file to .env_mineru and adjust the paths for your machine:
#   cp .env_mineru.example .env_mineru
# Then source it before running the pipeline:
#   source dependency_setup/.env_mineru

# Enable the MinerU CLI integration (required for real PDF processing).
export GLOSSAPI_MINERU_ALLOW_CLI=1

# Disable the stub backend in production (set to 1 only for tests/local dev).
export GLOSSAPI_MINERU_ALLOW_STUB=0

# Absolute path to the magic-pdf CLI binary.
export GLOSSAPI_MINERU_COMMAND=/path/to/glossAPI/dependency_setup/.venvs/mineru-py312/bin/magic-pdf

# MinerU execution mode: auto | cli | stub
export GLOSSAPI_MINERU_MODE=auto

# Set to 1 to skip Docling bootstrap (dev-only); use 0 for production.
export GLOSSAPI_SKIP_DOCLING_BOOT=1

# Absolute path to the MinerU tools config JSON (inside model_weights/).
export MINERU_TOOLS_CONFIG_JSON=/path/to/glossAPI/model_weights/mineru/magic-pdf.json
